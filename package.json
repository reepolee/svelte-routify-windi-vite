{
  "name": "svelte-app",
  "version": "1.1.0",
  "scripts": {
    "dev": "routify -c dev-vite ",
    "dev:windows": "npm run open & routify -c dev-vite ",
    "build": "routify -b && vite build --base=./",
    "build:br": "routify -b && vite build && npm run brotli:dist",
    "dev-vite": "vite",
    "serve-vite": "vite preview",
    "brotli:dist": "node br.js",
    "open": "start http://localhost:3000",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.0",
    "@roxi/routify": "^2.14.0",
    "@svitejs/vite-plugin-svelte": "^0.10.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/html-minifier": "^4.0.0",
    "axios": "^0.21.1",
    "brotli": "^1.3.2",
    "glob": "^7.1.6",
    "html-minifier": "^4.0.0",
    "open": "^7.4.2",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.5",
    "svelte-preprocess": "^4.6.9",
    "tslib": "^2.1.0",
    "vite": "^2.0.5",
    "vite-plugin-windicss": "^0.6.11",
    "mdsvex": "^0.8.9",
    "remark-slug": "^6.0.0",
    "routify-plugin-frontmatter": "^1.0.1"
  },
  "routify": {
    "extensions": [
      "md",
      "svx",
      "svelte"
    ],
    "plugins": {
      "routify-plugin-frontmatter": {}
    }
  }
}
